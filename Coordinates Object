public class Coordinates implements Comparator<Double>{
	int x;
	int y;
	int dim;
	double weight=0;
	public Coordinates(int first, int second, int third){
		x=first;
		y=second;
		dim=third;
		weight=(double)dim;
	}
	public void setThis(int i,int j)
	{
		this.x=i;
		this.y=j;
	}
	public boolean isEqual(Coordinates a, Coordinates b) {
		if(a.x==b.x && a.y==b.y) {
			return true;
		}	
		return false;
	}
	public int compare(Coordinates a, Coordinates b)
	{
		if(a.weight<=b.weight)
		{
			return 0;
		}
		else
		{
			return 1;
		}
	}
	public int compareTo(Coordinates a, Coordinates b) {
		if(isEqual(a,b)) {
			return 0;
		}
		return 1;	
	}
}
